    "@umijs/fabric": "^3.0.0",
    "@umijs/max": "^4.0.8",
    "@umijs/openapi": "^1.6.0",
    "carlo": "^0.9.46",
    "cross-env": "^7.0.3",
    "cross-port-killer": "^1.4.0",
    "detect-installer": "^1.0.2",
    "enzyme": "^3.11.0",
    "eslint": "^8.20.0",
