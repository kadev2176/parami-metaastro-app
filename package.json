    "jsdom-global": "^3.0.2",
    "lint-staged": "^13.0.3",
    "mockjs": "^1.1.0",
    "prettier": "^2.7.1",
    "puppeteer-core": "^15.4.2",
    "stylelint": "^14.9.1",
    "swagger-ui-react": "^4.13.0",
    "typescript": "^4.7.4",
